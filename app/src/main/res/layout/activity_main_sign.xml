<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#33FF96"
    android:orientation="vertical"
    tools:context=".activity.main_signActivity">


    <TextView
        android:id="@+id/TextView1"
        android:layout_width="wrap_content"
        android:layout_height="51dp"

        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="20dp"
        android:text="ExpenseManagerApp"
        android:textColor="#000000"
        android:textSize="40dp"
        android:textStyle="bold|italic"
        android:visibility="visible" />

    <ImageView
        android:id="@+id/googl"
        android:layout_width="359dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/TextView1"
        android:layout_alignRight="@+id/TextView1"
        android:layout_alignBottom="@+id/TextView1"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="-585dp"
        android:src="@drawable/googl" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/googl"
        android:layout_alignEnd="@+id/googl"
        android:layout_alignRight="@+id/googl"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="699dp"
        android:layout_marginEnd="136dp"
        android:layout_marginRight="136dp"
        android:layout_marginBottom="78dp"
        android:text="BY - "
        android:textSize="20sp"
        android:textStyle="bold|italic" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_alignEnd="@+id/textView"
        android:layout_alignRight="@+id/textView"
        android:layout_marginTop="-88dp"
        android:layout_marginEnd="-137dp"
        android:layout_marginRight="-137dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Raman Bansal" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editTextTextPersonName"
        android:layout_alignEnd="@+id/editTextTextPersonName"
        android:layout_alignRight="@+id/editTextTextPersonName"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="-1dp"
        android:layout_marginRight="-1dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nishant Kumar" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/googl"
        android:layout_marginBottom="-49dp"
        android:src="@drawable/icon" />


</RelativeLayout>